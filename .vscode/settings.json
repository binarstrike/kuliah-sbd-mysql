{
  "editor.fontFamily": "ComicShannsMono Nerd Font",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.fontWeight": "900",
  "[sql]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.formatOnSave": true,
    "files.eol": "\n",
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    }
  },
  "[php]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.formatOnSave": true,
    "files.eol": "\n",
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    }
  }
}
